{
  "name": "goupstrong",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "css-build": "npx sass sass/app.scss public/app.css",
    "css-watch": "npm run css-build -- --watch",
    "dev": "set NODE_OPTIONS='--inspect' && next dev",
    "build": "next build",
    "start": "NODE_ENV=production next start -p 80",
    "start-https": "NODE_ENV=production PORT=443 node server.js",
    "refresh": "git pull origin main && npm run css-build && npm run build && pm2 restart 0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "babel-eslint": "^10.1.0",
    "bulma": "^0.9.2",
    "cors": "^2.8.5",
    "date-fns": "^2.21.1",
    "eslint": "^7.25.0",
    "express": "^4.17.1",
    "googleapis": "^73.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.6",
    "mongodb-client-encryption": "^1.2.3",
    "netlify-cms-app": "^2.14.35",
    "next": "10.0.8",
    "next-auth": "^3.17.2",
    "nodemailer": "^6.5.0",
    "react": "17.0.1",
    "react-cookie": "^4.0.3",
    "react-dom": "17.0.1",
    "react-quill": "^1.3.5",
    "sanitize-html": "^2.5.2",
    "sass": "^1.32.8",
    "swr": "^0.5.3"
  }
}
